{
  "presets": [
    [
      "@babel/preset-env",
      {
        "loose": true,
        "targets": "> 1.5%, not dead, not op_mini all"
      }
    ]
  ],
  "plugins": [
    "@babel/plugin-transform-react-jsx",
    "@babel/plugin-proposal-optional-chaining",
    "@babel/plugin-proposal-nullish-coalescing-operator"
  ],
  "env": {
    "esm": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "loose": true,
            "modules": false,
            "targets": {
              "esmodules": true
            }
          }
        ]
      ]
    }
  }
}
