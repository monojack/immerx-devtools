{
  "name": "@immerx/devtools",
  "version": "0.1.0",
  "description": "DevTools middleware for Immer and Immerx",
  "private": true,
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "scripts": {
    "clean": "rimraf dist esm cjs umd",
    "start": "NODE_ENV=development webpack-dev-server",
    "build:ext": "NODE_ENV=production webpack -p --silent",
    "build:umd": "NODE_ENV=development rollup -c -o umd/immerx-devtools.js",
    "build:umd:min": "NODE_ENV=production rollup -c -o umd/immerx-devtools.min.js",
    "build:esm": "BABEL_ENV=esm babel src/index.js --out-file ${npm_package_module}",
    "build:cjs": "BABEL_ENV=cjs babel src/index.js --out-file ${npm_package_main}",
    "prebuild": "npm run clean",
    "build": "npm run build:umd && npm run build:umd:min && npm run build:esm && npm run build:cjs && npm run build:ext",
    "format": "prettier --write \"src/**/*.js\"",
    "lint": "eslint src"
  },
  "files": [
    "esm",
    "cjs",
    "umd"
  ],
  "homepage": "https://github.com/monojack/immerx-devtools",
  "bugs": "https://github.com/monojack/immerx-devtools/issues",
  "author": "Ionut Achim <achimvionut@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "@immerx/patchdiff": "^1.0.0",
    "classnames": "^2.2.6",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-object-view": "^0.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.0",
    "@babel/core": "^7.10.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.10.0",
    "@babel/plugin-transform-react-jsx": "^7.8.3",
    "@babel/preset-env": "^7.10.0",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@rollup/plugin-replace": "^2.3.2",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.3",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-standard": "^4.0.1",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.10.9",
    "rollup-plugin-analyzer": "^3.2.3",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^2.6.4",
    "rollup-plugin-terser": "^6.1.0",
    "rollup-plugin-visualizer": "^4.0.4",
    "sass": "^1.24.4",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "svg-inline-loader": "^0.8.2",
    "terser-webpack-plugin": "^3.0.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.10",
    "webpack-stylish": "^0.1.8"
  }
}
